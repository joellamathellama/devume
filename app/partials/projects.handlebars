<section class="projects-wrap">
  <div class="projects">
    <h2 class="section-head">Recent Projects</h2>
      {{#each projects}}
        <h6 class="section-sub-head">
          <a target="_blank" href="{{this.url}}" title="{{this.name}}"><strong>{{this.name}}</strong></a>
          <span class="position">{{this.position}}</span>  
        </h6>
        <p class="summary">{{this.summary}}</p>
        {{#if this.highlights}}
          <ul class="highlights">
            {{#each this.highlights}}
              <li>{{this}}</li>
            {{/each}}
          </ul>
        {{/if}}
      {{/each}}
  </div>
</section>